/**app.wxss**/
page{
  background:#575757;
  min-height: 100%;
}
.main_box{
}
.main_box__scroll{
  width:calc(100vw - 20px);
  margin:0 10px;
  padding:10px 0;
  background:#575757;
}

.main_list {
  display: flex;
  flex-wrap:wrap;
  justify-content:space-between;
  overflow:hidden;
}

.main_list__li{
  width:46vw;
  height:56vw;
  text-align: center;
  background-size:100% auto;
  background-repeat: no-repeat;
  position: relative;
}

.main_list__bg{
  position: absolute;
  width:46vw;
  height:46vw;
  left:0;
  top:0;
}

.main_list__bg2{
  position: absolute;
  width:42.4vw;
  height:42.4vw;
  left:1.7vw;
  top:1.7vw;
  background:#fff;
}


.main_list__img{
  position: absolute;
  width:39vw;
  height:39vw;
  left:3.4vw;
  top:3.4vw;
}
.main_list__text{
  position:absolute;
  width:100%;
  left:0;
  bottom:3vw;
  color:#fff;
  font-size:16px;
}







